{
  "name": "cigna-interview",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "install-dependencies": "npm install -g jest && npm run backend-dependencies && npm run frontend-dependencies",
    "start": "node --experimental-json-modules ./bin/www",
    "test": "jest tests",
    "testServerClient": "npm test && npm run testClient",
    "startClient": "cd client && npm start",
    "testClient": "cd client && npm test",
    "frontend-dependencies": "cd client && npm install",
    "backend-dependencies": "npm install"
  },
  "jest": {
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "babel-jest": "^26.6.0",
    "babel-polyfill": "^6.26.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "babel-jest": "26.6.3",
    "jest": "26.6.0"
  }
}
